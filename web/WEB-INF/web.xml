<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--访问统计-->
    <servlet-mapping>
        <servlet-name>AccessServlet</servlet-name>
        <url-pattern>/Access</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AccessServlet</servlet-name>
        <servlet-class>main.notes.servlet.AccessServlet</servlet-class>
    </servlet>

    <!--所有文件详情-->
    <servlet-mapping>
        <servlet-name>DocDetailServlet</servlet-name>
        <url-pattern>/docs/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DocDetailServlet</servlet-name>
        <servlet-class>main.notes.servlet.DocDetailServlet</servlet-class>
    </servlet>

    <!--食锦记 列表-->
    <servlet-mapping>
        <servlet-name>FoodListServlet</servlet-name>
        <url-pattern>/FoodList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FoodListServlet</servlet-name>
        <servlet-class>main.notes.servlet.FoodListServlet</servlet-class>
    </servlet>

    <!--心晴 列表-->
    <servlet-mapping>
        <servlet-name>FeelListServlet</servlet-name>
        <url-pattern>/FeelList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FeelListServlet</servlet-name>
        <servlet-class>main.notes.servlet.FeelListServlet</servlet-class>
    </servlet>

    <!--轶事 列表-->
    <servlet-mapping>
        <servlet-name>StoryListServlet</servlet-name>
        <url-pattern>/StoryList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StoryListServlet</servlet-name>
        <servlet-class>main.notes.servlet.StoryListServlet</servlet-class>
    </servlet>
</web-app>